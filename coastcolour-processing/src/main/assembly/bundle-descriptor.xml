    <!-- The upper bound of the valid value range. The lower bound is always 0. -->
    <adgUpperBound>1.0</adgUpperBound>
    <!-- If selected the source remote reflectances are divided by PI -->
    <divideByPI>true</divideByPI>
</parameters>]]></defaultParameters>
